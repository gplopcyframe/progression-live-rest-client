{
    "name": "cyframe/progression-live-rest-client",
    "version": "0.0.1",
    "description": "Swagger generated ProgressionLIVE REST API Client.",
    "license": "proprietary",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "ProgressionLive\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "ProgressionLive\\" : "test/" }
    }
